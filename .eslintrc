{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "prettier",
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "semi": ["error", "always"],
        "indent": ["error", 2, { "SwitchCase": 1 }],
        "comma-dangle": "off",
        "import/no-unresolved": "off",
        "import/prefer-default-export": "off",
        "object-curly-newline": "off",
	"linebreak-style": "off"
    }
}
